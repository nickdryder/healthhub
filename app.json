{
  "expo": {
    "name": "HealthHub",
    "slug": "healthhub",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "healthhub",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nickdryder.healthhub",
      "infoPlist": {
        "NSHealthShareUsageDescription": "This app reads your health data to provide personalized insights and track your wellness.",
        "NSHealthUpdateUsageDescription": "This app saves workout and health data to Apple Health.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "buildNumber": "9"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.nickdryder.healthhub"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "@kingstinct/react-native-healthkit",
        {
          "NSHealthShareUsageDescription": "This app reads your health data to provide personalized insights.",
          "NSHealthUpdateUsageDescription": "This app can save workout data to Apple Health.",
          "background": false
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "1d21cdc7-6fc2-4ad2-aea7-6a746b1ca7c7"
      },
      "router": {}
    },
    "owner": "nickdryder"
  }
}
